{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020"],
		"module": "ESNext",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"noUncheckedIndexedAccess": true
	}
}
