{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "tsBuildInfoFile": "../node_modules/.tmp/tsconfig.server.tsbuildinfo",
        "moduleResolution": "node",
        "module": "CommonJS",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "outDir": "dist",
        "rootDir": "src",
        "noEmit": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
